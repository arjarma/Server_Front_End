<button type="button" class="btn btn-primary btn-lg" value="lst" onclick="lis();">LIST </button> 
 
<button type="button" class="btn btn-primary btn-lg" value="" onclick="delet();" >DELETE</button>
<button type="button" class="btn btn-primary btn-lg" value="proc"onclick="proc();">PROCESS </button>
