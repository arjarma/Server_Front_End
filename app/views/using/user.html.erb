<script type="text/javascript">
function lis()
{
<%=require 'redis'%>
<%=redis = Redis.new(:host => 'localhost',:port => '6379')%>
<%=require 'securerandom'%>
<%=String uni=SecureRandom.hex(11)%>
<%=redis.lpush('process',uni);%>
}
</script>
<script type="text/javascript">
function delet()
{
<%=require 'redis'%>
<%=redis = Redis.new(:host => 'localhost',:port => '6379')%>

<%=redis.blpop('process','1');%>//delete
}
</script>
<script type="text/javascript">
function proc()
{
<%=require 'redis'%>
<%=redis = Redis.new(:host => 'localhost',:port => '6379')%>
<%=redis.lpop('process');%>//process
}
</script>
<script type="text/javascript">
function delall()
{
<%=require 'redis'%>
<%=redis = Redis.new(:host => 'localhost',:port => '6379')%>

<%=redis.flushall;%>//delete all
}
</script>
<script type="text/javascript">
function queue()
{
<%=require 'redis'%>
<%=redis = Redis.new(:host => 'localhost',:port => '6379')%>
<%=red=redis.llen('process');%>//no of processes in queue
}
</script>



<div style="background : silver">
